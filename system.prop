#
# system.prop for galaxys4gmtd
#

rild.libpath=/system/lib/libsec-ril.so
rild.libargs=-d /dev/ttyS0
ro.sf.lcd_density=240

# hardware info
ro.device.cpu=1 GHz ARM Cortex-A8
ro.device.gpu=PowerVR SGX 540
ro.device.rear_cam=5 MP
ro.device.front_cam=0.3 MP
ro.device.screen_res=480x800

# Some BattMods
# pm.sleep_mode=1
ro.ril.disable.power.collapse=0


# CWM key repeat
ro.cwm.enable_key_repeat=true
ro.cwm.repeatable_keys=115,114,139,217
